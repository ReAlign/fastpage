{
  "name": "fastpage",
  "version": "1.3.0",
  "description": "a tool to quickly generate front-end pages and associated files",
  "main": ".eslintrc.js",
  "bin": {
    "fastpage": "bin/fastpage.js",
    "fp": "bin/fastpage.js"
  },
  "scripts": {
    "precommit": "lint-staged",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ReAlign/fastpage.git"
  },
  "keywords": [
    "fast",
    "page",
    "generate file",
    "quickly",
    "tool",
    "nodejs",
    "auto"
  ],
  "lint-staged": {
    "linters": {
      "bin/*.js": [
        "eslint --fix",
        "git add"
      ],
      "lib/**/*.js": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "dependencies": {
    "commander": "^2.11.0",
    "chalk": "^2.1.0",
    "inquirer": "^3.3.0",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "babel-eslint": "^8.0.1",
    "husky": "^0.13.4",
    "lint-staged": "^3.6.1",
    "jest": "^21.2.1",
    "codecov": "^2.3.0"
  },
  "author": "ReAlign",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ReAlign/fastpage/issues"
  },
  "homepage": "https://github.com/ReAlign/fastpage#readme"
}
